{
  "extends": [
    "standard",
    "standard-react",
    "standard-flow"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "impliedStrict": true,
      "globalReturn": false,
      "jsx": true
    }
  },
  "rules": {
    "react/prop-types": 0
  }
}
